	"default_routes": {
		"create": {
			"method": "post",
            "route": "/",
            "validation": {
				"all except id": {
					"where": ["body"],
					"check": ["exists", "not", "isEmpty", "trim"]
				},
				"id": {
					"where": ["body"],
					"check": ["not", "exists"]
				}
            }
		},
		"update": {
			"method": "put",
			"route": "/:id",
			"validation": {
				"all except id":{
					"where": ["body"],
					"check": ["optional", "not", "isEmpty", "trim"]
				},
				"id": {
					"where": ["param", "body"],
                	"text": ["You must introduce a valid id", "You cannot upload the id of a row"],
                	"check": [["exists", "not", "isEmpty", "isInt"], ["not", "exists"]]
				}
			}
		},
		"findAll": {
			"method": "get",
			"route": "/",
			"validation": {
				"page": {
	                "where": ["query"],
	                "text": ["Page query param must be an integer"],
	                "check": ["optional", "not", "isEmpty", "isInt"]
              	}
			}
		}
	},
	"sql2sequelize": {
		"int": "INTEGER",
		"char": "STRING",
		"varchar": "STRING",
		"text": "TEXT",
		"mediumtext": "TEXT('medium')",
		"longtext": "TEXT('long')",
		"date": "DATE",
		"boolean": "BOOLEAN"
	}
