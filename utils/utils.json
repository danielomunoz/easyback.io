	"default_mongo_package_json": {
		"name": "default",
  		"version": "v1.0.0",
  		"description": "no desc",
  		"main": "server.js",
  		"scripts": {
    		"test": "echo \"Error: no test specified\" && exit 1",
    		"npm run start": "node server.js"
  		},
  		"author": "me",
  		"license": "ISC",
  		"dependencies": {
  			"body-parser": "^1.19.0",
  			"express": "^4.17.1",
  			"express-validator": "^6.6.1",
    		"jsonwebtoken": "^8.5.1",
